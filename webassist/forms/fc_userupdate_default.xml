<wa_form name="User Update" id="UserUpdate" submit_text="Update" send_mail="false" use_upload="false">
	<wa_form_fieldset label="User Update" id="User Update">
		<wa_fieldset_description>Required *</wa_fieldset_description>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Please enter a value.</wa_field_set_validation>
							<wa_field_reference src="text_large.xml" label="Email Address" defaultvalue="%3C?php%20echo%20$row_SecurityAssistuserlogin%5B%22email%22%5D;%20?%3E" validation="{ required: true, type: 'email', is_unique: true, connection: 'local', table: 'user_login', keyColumn: 'id', keyType: 'none,none,NULL', keyValue: '%22.((isset($_SESSION%5B%22SecurityAssist_id%22%5D))?$_SESSION%5B%22SecurityAssist_id%22%5D:%220%22)%20%20.%22', filterColumn: 'email', filterType: '%27,none,%27%27', filterValue: '%22.((isset($_POST%5B%22%7BField.Name%7D%22%5D))?$_POST%5B%22%7BField.Name%7D%22%5D:%22%22)%20%20.%22' }" serverValidation="email|unique" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Password strength requirement not met. @@strengthmessage@@</wa_field_set_validation>
							<wa_field_reference src="password_large.xml" label="Password" validation="{ required: false, isConfirm: false, minAlphaChars: 3, maxAlphaChars: 500, minNumbers: 2, maxNumbers: 500, minSpecialChars: 0, maxSpecialChars: 0, minChars: 6, maxChars: 500, isRequired: false, is_required: false }" serverValidation="|entrylength" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>A value is required.</wa_field_set_validation>
							<wa_field_reference src="password_large.xml" label="Confirm" validation="{ required: false, isConfirm: true, isRequired: false, is_required: false }" serverValidation="confirm" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Please enter a value.</wa_field_set_validation>
							<wa_field_reference src="text_medium.xml" label="First Name" defaultvalue="%3C?php%20echo%20$row_SecurityAssistuserlogin%5B%22firstName%22%5D;%20?%3E" validation="{ type: 'none' }" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Please enter a value.</wa_field_set_validation>
							<wa_field_reference src="text_medium.xml" label="Last Name" defaultvalue="%3C?php%20echo%20$row_SecurityAssistuserlogin%5B%22lastName%22%5D;%20?%3E" validation="{ type: 'none' }" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Please enter a value.</wa_field_set_validation>
							<wa_field_reference src="text_medium.xml" label="Handle/Nickname" defaultvalue="%3C?php%20echo%20$row_SecurityAssistuserlogin%5B%22user_handle%22%5D;%20?%3E" validation="{ required: true, type: 'none', is_unique: true, connection: 'local', table: 'user_login', keyColumn: 'id', keyType: 'none,none,NULL', keyValue: '%22.((isset($_SESSION%5B%22SecurityAssist_id%22%5D))?$_SESSION%5B%22SecurityAssist_id%22%5D:%220%22)%20%20.%22', filterColumn: 'user_handle', filterType: '%27,none,%27%27', filterValue: '%22.((isset($_POST%5B%22%7BField.Name%7D%22%5D))?$_POST%5B%22%7BField.Name%7D%22%5D:%22%22)%20%20.%22' }" serverValidation="required|unique" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
	</wa_form_fieldset>
</wa_form>