<wa_form name="Reset Password" id="ResetPassword" submit_text="Reset" send_mail="false" use_upload="false">
	<wa_form_fieldset label="Reset Password" id="Reset Password">
		<wa_fieldset_description>Required *</wa_fieldset_description>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Please enter a value.</wa_field_set_validation>
							<wa_field_reference src="text_large.xml" label="Email Address" validation="{ required: true, type: 'email' }" serverValidation="email" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>Password strength requirement not met. @@strengthmessage@@</wa_field_set_validation>
							<wa_field_reference src="password_large.xml" label="Password" validation="{ required: true, isConfirm: false, minAlphaChars: 3, maxAlphaChars: 500, minNumbers: 2, maxNumbers: 500, minSpecialChars: 0, maxSpecialChars: 0, minChars: 6, maxChars: 500, isRequired: true }" serverValidation="required|entrylength" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference label="" continueLine="false">
				<wa_field_group name="">
					<wa_field_line>
						<wa_field_column>
							<wa_field_set_validation>A value is required.</wa_field_set_validation>
							<wa_field_reference src="password_large.xml" label="Confirm" validation="{ required: true, isConfirm: true, isRequired: true }" serverValidation="confirm" undefinedundefined continueLine="false"></wa_field_reference>
						</wa_field_column>
					</wa_field_line>
				</wa_field_group>
			</wa_form_group_reference>
		</wa_form_line>
		<wa_form_line>
			<wa_form_group_reference group="Security.xml" src="Security.xml" label="" continueLine="false"></wa_form_group_reference>
		</wa_form_line>
	</wa_form_fieldset>
</wa_form>