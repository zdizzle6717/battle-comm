{
  "name": "battle-comm",
  "version": "2.0.0",
  "description": "This is a universal javascript app built with react on the server side and client site demonstrated with simple movie data",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-client": "babel-watch server-client/index.js",
    "start-client": "forever start -c 'node_modules/.bin/babel-node' server-client/index.js",
    "stop-client": "forever stop 'server-client/index.js'",
    "watch-server": "babel-watch server/index.js",
    "start-server": "babel server/index.js --out-file server/index-compiled.js && pm2 start server/index-compiled.js --name='BattleCommAPI'",
    "stop-server": "pm2 stop 'BattleCommAPI'",
    "build": "node build-style.js && NODE_ENV=development node_modules/.bin/webpack -d --config webpack.dev.js",
    "build-prod": "node build-style.js && NODE_ENV=production node_modules/.bin/webpack -p --config webpack.prod.js",
    "style": "node build-style.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zdizzle6717/universal-react-movie-app.git"
  },
  "keywords": [
    "javascript",
    "universal",
    "react",
    "starter",
    "kit",
    "app",
    "reactjs",
    "router",
    "express",
    "hapijs"
  ],
  "author": "Zack Anselm",
  "license": "MIT",
